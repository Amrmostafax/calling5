{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "functions": {
        "app/api/**/*.ts": {
            "maxDuration": 30,
            "memory": 1024
        }
    },
    "env": {
        "GOOGLE_API_KEY": "@google-api-key"
    }
}